    "url": "https://github.com/tbranyen/todomvc/issues"
  },
  "homepage": "https://github.com/tbranyen/todomvc#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "derequire": "^2.0.3",
    "mocha": "^2.4.5",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "diffhtml": "^0.8.2",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "todomvc-app-css": "^2.0.6",
    "todomvc-common": "^1.0.2"
